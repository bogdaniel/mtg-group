{% extends '/templates/base.html.twig' %}
{% block load_text %}
    {% include 'templates/pages/multigama-group/_partial/_multigama-tech-loader.html.twig' %}
{% endblock %}
{% block header %}
    {% include 'templates/pages/_partial/_header.html.twig' with {'multiGamaGroupMenu': multiGamaGroupMenu, 'logoUrl': logoUrl} %}
{% endblock %}
{% block body %}
    <!-- Banner  -->
    <div class="dz-bnr-inr style-2 dz-bnr-inr-md overlay-black-middle"
         style="background-image: url('{{ asset('assets/images/poza-1.jpg') }}');">
        <div class="container">
            <div class="dz-bnr-inr-entry">
                <!-- Breadcrumb Row -->
                <nav aria-label="breadcrumb" class="breadcrumb-row">
                    <ul class="breadcrumb">
                        <li class="breadcrumb-item"><a href="#">{% trans %}Acasa{% endtrans %}</a></li>
                        <li class="breadcrumb-item">{% trans %}Proiecte{% endtrans %}</li>
                    </ul>
                </nav>
                <!-- Breadcrumb Row End -->
            </div>
        </div>
        <div class="shap-bx">
            <div class="bg-shape-left"></div>
            <div class="bg-shape-right"></div>
        </div>
    </div>
    <!-- Banner End -->

    <!-- Our Portfolio -->
    <div class="under-construct" style="background-image: url( {{ asset('assets/images/water-pumps.png') }} ); background-position: center;">
        <div class="inner-box">
            <div class="logo-header logo-dark">
                <a href="index.html"><img src="images/logo.png" alt=""></a>
            </div>
            <div class="dz-content">
                <h2 class="dz-title">{% trans %}Pagina In{% endtrans %} <br><span class="text-primary">{% trans %}Constructie{% endtrans %}</span></h2>
            </div>
        </div>
    </div>
{% endblock %}
