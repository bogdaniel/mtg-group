<!-- Header -->
<header class="site-header mo-left header style-1">
    <!-- Header Top Bar -->
    <div class="top-bar bg-secondary text-white">
        <div class="container">
            <div class="dz-topbar-inner d-flex justify-content-between align-items-center">
                <div class="dz-topbar-left">
                    <ul>
                        <li><i class="fas fa-map-marker-alt"></i> Str. 7 Drumuri, Nr. 9C, Sector 3, Bucure»ôti</li>
                        <li><i class="far fa-envelope"></i>office@multigama.ro</li>
                    </ul>
                </div>
                <div class="dz-topbar-right">
                    <ul class="dz-social">
                        <li><a class="fab fa-facebook-f" href="https://www.facebook.com/" target="_blank"></a></li>
                        <li><a class="fab fa-instagram" href="https://www.instagram.com/multigama.service" target="_blank"></a>
                    </ul>
                </div>
            </div>
        </div>
    </div>

    <!-- Main Header -->
    <div class="sticky-header main-bar-wraper navbar-expand-lg text-black">
        <div class="main-bar clearfix d-none d-sm-block">
            <div class="container clearfix bg-white">
                <div class="row">
                    <div class="col-8 offset-2">
                        <nav class="navbar navbar-expand-lg navbar-light">
                            <div class="container-fluid">
                                <div class="collapse navbar-collapse justify-content-between" id="navbarNav">
                                    <!-- Added 'justify-content-between' class -->
                                    <ul class="navbar-nav w-100 d-flex justify-content-between">
                                        <!-- Added 'w-100', 'd-flex' and 'justify-content-between' classes -->
                                        <li class="nav-item">
                                            <a aria-current="page" class="nav-link active" href="{{ path('static_page',{'pageName': 'multigama-tech-home'}) }}
">MULTIGAMA TECH</a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="nav-link" href="#">MULTIGAMA SERVICE</a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="nav-link" href="#">EQ FIRE</a>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
        <div class="main-bar clearfix">
            <div class="container clearfix bg-white">
                <!-- Website Logo -->
                <div class="logo-header mostion logo-dark">
                    <a href="/">
                        <img src="{{ asset_image('assets/images/' ~ logo) }}" alt="">
                    </a>
                </div>
                <!-- Nav Toggle Button -->
                <button class="navbar-toggler collapsed navicon justify-content-end" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNavDropdown" aria-controls="navbarNavDropdown" aria-expanded="false" aria-label="Toggle navigation">
                    <span></span>
                    <span></span>
                    <span></span>
                </button>
                <!-- Extra Nav -->
                <div class="extra-nav">
                    <div class="extra-cell">
                        <a href="#" class="btn shadow-primary btn-secondary btn-quote">
                            <span>CERE OFERTA</span>
                        </a>
                    </div>
                </div>
                <!-- Extra Nav -->
                <!-- Quik Search -->
                <!--
                <div class="dz-quik-search">
                    <form action="#">
                        <input name="search" value="" type="text" class="form-control" placeholder="Enter Your Keyword ...">
                        <span  id="quik-search-remove"><i class="ti-close"></i></span>
                    </form>
                </div>
                -->
                <!-- Quik Search -->
                <div class="header-nav navbar-collapse collapse justify-content-end" id="navbarNavDropdown">
                    <div class="logo-header logo-dark">
                        <a href="/">
                            <img src="{{ asset_image('assets/images/' ~ logo) }}" alt="">
                        </a>
                    </div>
                    <ul class="nav navbar-nav">
                        {% for item in multiGamaGroupMenu %}
                            {% if item.sub_menu is defined %}
                                <li class="active sub-menu-down">
                                <a href="javascript:;">{{ item.name }}</a>
                                <ul class="sub-menu">
                                        {% for subItem in item.sub_menu %}
                                                <li class="new-page">
                                                    <a href="{{ subItem.path }}">{{ subItem.name }}</a>
                                                </li>
                                        {% endfor %}
                                    </ul>
                            {% else %}
                                <li class="active">
                                <a href="{{ item.path }}">{{ item.name }}</a>

                            {% endif %}
                            </li>
                        {% endfor %}
                    </ul>
                    <div class="dz-social-icon">
                        <ul>
                            <li><a class="fab fa-facebook-f" href="https://www.facebook.com/" target="_blank"></a></li>
                            <li><a class="fab fa-instagram" href="https://www.instagram.com/multigama.service" target="_blank"></a>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Main Header End -->

</header>
<!-- Header End -->
