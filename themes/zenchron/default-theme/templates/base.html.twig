{#    <script src="{{ asset('/assets/vendor/aos/aos.javascript') }}"></script><!-- AOS -->#}
    <script src="{{ asset('/assets/javascript/dz.carousel.javascript') }}"></script><!-- OWL-CAROUSEL -->
{#    <script src="{{ asset('/assets/javascript/dz.ajax.javascript') }}"></script><!-- AJAX -->#}
{#    <script src="{{ asset('/assets/javascript/custom.javascript') }}"></script><!-- CUSTOM JS -->#}

</head>
<body id="bg" class="theme-rounded">
<div id="loading-area">
    <div class="loading-inner style-1">
        <div class="wrapper">
            {% block loading_wrapper %}{% endblock %}
        </div>
        <div class="load-text">
            {% block load_text %}{% endblock %}
        </div>
    </div>
</div>
<div class="page-wraper">
    {% block header %}{% endblock %}
    <div class="page-content bg-white">
        {% block body %}{% endblock %}
    </div>
    {% include 'templates/pages/_partial/_footer.html.twig' %}
    <button class="scroltop icon-up" type="button"><i class="fas fa-arrow-up"></i></button>
</div>
</body>
</html>
