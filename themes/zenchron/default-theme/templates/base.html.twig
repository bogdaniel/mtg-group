    <!-- Meta -->

    <!-- Favicon icon -->
{#    <link rel="icon" type="image/png" href="images/favicon.png">#}
    <!-- Favicon icon -->


    <!-- Stylesheet -->
    <link href="{{ asset('/assets/vendor/magnific-popup/magnific-popup.min.css') }}" rel="stylesheet">
    <link href="{{ asset('/assets/vendor/swiper/swiper-bundle.min.css') }}" rel="stylesheet">
    <link href="{{ asset('/assets/vendor/perfect-scrollbar/css/perfect-scrollbar.css') }}" rel="stylesheet">
    <link href="{{ asset('/assets/vendor/lightgallery/css/lightgallery.min.css') }}" rel="stylesheet">
    <link href="{{ asset('/assets/vendor/aos/aos.css') }}" rel="stylesheet">

    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link
        href="https://fonts.googleapis.com/css2?family=Montserrat:wght@100;200;300;400;500;600;700;800;900&family=Roboto:wght@100;300;400;500;700;900&family=Rubik:wght@300;400;500;600;700;800;900&display=swap"
        rel="stylesheet">

    <!-- Custom Stylesheet -->
    <link rel="stylesheet" href="{{ asset('/assets/css/style.css') }}">
    <link rel="stylesheet" class="skin" href="{{ asset('/assets/css/skin/' ~ skinVersion) }}">

    {% block stylesheets %}
        {{ encore_entry_link_tags('app_js') }}
    {% endblock %}

    {% block javascripts %}
{#        {{ encore_entry_script_tags('app_css') }}#}
    {% endblock %}
    <!-- JAVASCRIPT FILES ========================================= -->
    <script src="{{ asset('/assets/javascript/jquery.min.javascript') }}"></script><!-- JQUERY.MIN JS -->
    <script src="{{ asset('/assets/vendor/bootstrap/javascript/bootstrap.bundle.min.javascript') }}"></script><!-- BOOTSTRAP.MIN JS -->
    <script src="{{ asset('/assets/vendor/magnific-popup/magnific-popup.javascript') }}"></script><!-- MAGNIFIC POPUP JS -->
    <script src="{{ asset('/assets/vendor/counter/waypoints-min.javascript') }}"></script><!-- WAYPOINTS JS -->
    <script src="{{ asset('/assets/vendor/counter/counterup.min.javascript') }}"></script><!-- COUNTERUP JS -->
{#    <script src="{{ asset('/assets/vendor/masonry/isotope.pkgd.min.javascript') }}"></script><!-- ISOTOPE -->#}
    <script src="{{ asset('/assets/vendor/imagesloaded/imagesloaded.javascript') }}"></script><!-- IMAGESLOADED -->
{#    <script src="{{ asset('/assets/vendor/masonry/masonry-4.2.2.javascript') }}"></script><!-- MASONRY -->#}
