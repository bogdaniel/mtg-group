<tr>
    <td>{{ key }}</td>
    <td>
        {% if value is iterable %}
            <ul>
                {% for subKey, subValue in value %}
                {% include 'table_row.html.twig' with {'key': subKey, 'value': subValue} %}
                {% endfor %}
            </ul>
        {% elseif key in ['Require', 'Require Dev'] %}
            {{ value }} ({{ value in installedPackages ? 'Installed' : 'Not Installed' }})
        {% else %}
            {{ value }}
        {% endif %}
    </td>
</tr>
